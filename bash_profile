osascript -e "tell application \"Terminal\" to set normal text color of window 1 to {0, 63993, 0}"
osascript -e "tell application \"Terminal\" to set background color of window 1 to {10000,10000,10000}"
osascript -e "tell application \"Terminal\" to set the font size of window 1 to 18"
export PATH="$PATH:$HOME/Scripts/bash-scripts"